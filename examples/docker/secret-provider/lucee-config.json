{
	"secretProvider": {
		"env": {
			"class": "lucee.runtime.security.EnvVarSecretProvider",
			"custom": {
				"caseSensitive": false
			}
		},
		"sysprop": {
			"class": "lucee.runtime.security.SystemPropSecretProvider",
			"custom": {
				"caseSensitive": false
			}
		},
		"json": {
			"class": "lucee.runtime.security.FileSecretProvider",
			"custom": {
				"type": "json",
				"file": "/opt/lucee/server/artifacts/secrets.json",
				"caseSensitive": false
			}
		},
		"sm": {
			"class": "org.lucee.extension.aws.sm.AWSSecretManagerProvider",
			"maven": "org.lucee:aws-sm:1.1.0.0-BETA",
			"custom": {
				"accessKeyId": "dummy",
				"secretKey": "dummy",
				"region": "us-east-1",
				"endpoint": "http://localstack:4566",
				"jsonTraversal": true,
				"timeout": 5000
			}
		},
		"ps": {
			"class": "org.lucee.extension.aws.ssm.AWSParameterStoreProvider",
			"maven": "org.lucee:aws-sm:1.1.0.0-BETA",
			"custom": {
				"accessKeyId": "dummy",
				"secretKey": "dummy",
				"region": "us-east-1",
				"endpoint": "http://localstack:4566",
				"jsonTraversal": true,
				"timeout": 5000
			}
		}
	}
}