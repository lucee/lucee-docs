	"secretProvider": {
		"env": {
			"class": "lucee.runtime.security.EnvVarSecretProvider",
			"custom": {
				"caseSensitive": false
			}
		},
		"json": {
			"class": "lucee.runtime.security.FileSecretProvider",
			"custom": {
